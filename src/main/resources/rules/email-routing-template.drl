template header
id
ifcondition
thencondition

import org.yuenio.droolSandbox.model.Email;
import org.yuenio.droolSandbox.model.RoutingRule;

template "tmp2"

rule "@{id}"
  dialect "mvel"
  when
    EmailObject : Email(subject == "@{ifcondition}");
  then
    EmailObject.setTeamAssigned("@{thencondition}");
end

end template